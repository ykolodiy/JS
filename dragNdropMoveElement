<script>




function allowDrop(ev) {

var s = ev.target.hasChildNodes();
//console.log(ev.target.id + " " +Boolean(s));
if (Boolean(s))
{
ev.preventDefault();
}





}

function drag(ev) {
    ev.dataTransfer.setData("text", ev.target.id);
}

function drop(ev) {
 
var s = ev.target.hasChildNodes();

if (Boolean(s))
{
ev.preventDefault();

    var data = ev.dataTransfer.getData("text");
    ev.target.appendChild(document.getElementById(data));
    
   }
    
    
    
}

